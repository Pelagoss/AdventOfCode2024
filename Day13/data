94a+22b=8400
94a=(8400-22b)
a=8400/94 - 22b/94

34a+67b=5400
34a=5400-67b
a=5400/34-67b/34

8400/94 - 22b/94 = 5400/34-67b/34
34*8400/94 - 34*22b/94 = 5400-67b
34*8400 - 34*22b = 94*5400 - 94*67b
-34*22b=94*5400 - 34*8400 - 94*67b
94*67b-34*22b=94*5400 - 34*8400
(94*67-34*22)b = 94*5400 - 34*8400

// A et B
b=((94*5400)-(34*8400))/(94*67-34*22)
a=(8400/94)-(22*(((94*5400)-(34*8400))/(94*67-34*22))/94)